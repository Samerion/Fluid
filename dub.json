{
	"authors": [
		"Artha"
	],
	"configurations": [
		{
			"name": "default",
			"targetType": "staticLibrary"
		},
		{
			"name": "dynamic",
			"targetType": "dynamicLibrary",
			"dflags-posix-dmd": [
				"-defaultlib=libphobos2.so"
			],
			"dflags-ldc": [
				"--link-defaultlib-shared"
			],
			"libs-windows": [
				"LIBCMT"
			]
		},
		{
			"name": "source",
			"targetType": "sourceLibrary"
		},
		{
			"libs": [
				"raylib"
			],
			"name": "unittest",
			"targetType": "library",
			"versions": [
				"Fluid_HeadlessOutput"
			],
			"dependencies": {
				"silly": "~>1.1.1"
			}
		},
		{
			"dflags-ldc": [
				"--ftime-trace",
				"--ftime-trace-file=build/trace.json"
			],
			"name": "time-trace",
			"targetType": "library"
		}
	],
	"copyFiles-windows": [
		"$PACKAGE_DIR/libraries/windows/freetype.dll",
		"$PACKAGE_DIR/libraries/windows/raylib.dll"
	],
	"copyright": "Copyright © 2021–2024, Artha",
	"dependencies": {
		"arsd-official:image_files": {
			"optional": true,
			"version": "~>11.3"
		},
		"bindbc-freetype": "~>1.1.1",
		"bindbc-loader": "~>1.1.3",
		"elemi": {
			"optional": true,
			"version": "~>1.2.2"
		},
		"raylib-d": {
			"default": true,
			"optional": true,
			"version": "~>5"
		}
	},
	"description": "A straightforward and easy to use GUI library.",
	"excludedSourceFiles": [
		"source/fluid/module_view.d"
	],
	"lflags-linux": [
		"-L$PACKAGE_DIR/libraries/ubuntu"
	],
	"lflags-osx": [
		"-L$PACKAGE_DIR/libraries/macos"
	],
	"lflags-windows": [
		"/LIBPATH:$PACKAGE_DIR/libraries/windows"
	],
	"libs": [
		"freetype"
	],
	"license": "BSL",
	"name": "fluid",
	"stringImportPaths": [
		"resources"
	],
	"subConfigurations": {
		"bindbc-freetype": "staticBC"
	},
	"subPackages": [
		"./tour",
		{
			"dependencies": {
				"fluid": {
					"path": "."
				},
				"libdparse": "~>0.23.2"
			},
			"libs": [
				"raylib"
			],
			"name": "showcase",
			"preBuildCommands": [
				"echo Deprecated: fluid:showcase has been renamed to fluid:tour"
			],
			"sourcePaths": [
				"tour"
			],
			"targetName": "fluid-tour",
			"targetPath": "build",
			"targetType": "executable"
		},
		{
			"name": "init-exec",
			"sourceFiles": [
				"source/fluid/dub_template.d"
			],
			"sourcePaths": [],
			"targetPath": "build",
			"targetType": "executable",
			"versions": [
				"Fluid_InitExec"
			]
		},
		{
			"dependencies": {
				"fluid": {
					"path": "."
				},
				"fluid-tree-sitter": "~>0.1.6",
				"fluid-tree-sitter:d": "~>0.1.6"
			},
			"name": "module-view",
			"sourceFiles": [
				"source/fluid/module_view.d"
			],
			"sourcePaths": [],
			"subConfigurations": {
				"fluid": "dynamic"
			},
			"configurations": [
				{
					"name": "linux-dynamic",
					"platforms": [
						"linux"
					],
					"targetType": "staticLibrary",
					"dflags-posix-dmd": [
						"-defaultlib=libphobos2.so"
					],
					"dflags-ldc": [
						"--link-defaultlib-shared"
					]
				},
				{
					"name": "static",
					"targetType": "staticLibrary",
					"preRunCommands-windows": [
						"ldd build/fluid-module-view-test-static.exe"
					]
				}
			],
			"targetPath": "build",
			"targetType": "library",
			"versions": [
				"Fluid_ModuleView"
			]
		}
	],
	"targetPath": "build"
}