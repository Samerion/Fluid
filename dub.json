{
	"authors": [
		"Artha"
	],
	"buildTypes": {
		"docs": {
			"buildOptions": [
				"syntaxOnly"
			],
			"dflags": [
				"-Dddocs",
				"-op",
				"-X",
				"-Xf=docs.json"
			],
			"postBuildCommands": [
				"sed -i.bak 's:href=\"docs/:href=\"/docs/:g' *docs/source/fluid/**.html"
			],
			"sourceFiles": [
				"resources/theme.ddoc"
			],
			"versions": [
				"Fluid_Docs"
			]
		},
		"serve-docs": {
			"buildOptions": [
				"syntaxOnly"
			],
			"dflags": [
				"-Dddocs",
				"-op",
				"-X",
				"-Xf=docs.json"
			],
			"postBuildCommands": [
				"sed -i.bak 's:href=\"docs/:href=\"/docs/:g' *docs/source/fluid/**.html",
				"$DUB run servefolder -- -d \"$PACKAGE_DIR\""
			],
			"sourceFiles": [
				"resources/theme.ddoc"
			],
			"versions": [
				"Fluid_Docs"
			]
		}
	},
	"configurations": [
		{
			"name": "default",
			"targetType": "staticLibrary"
		},
		{
			"name": "source",
			"targetType": "sourceLibrary"
		},
		{
			"dflags-ldc": [
				"--link-defaultlib-shared"
			],
			"dflags-posix-dmd": [
				"-defaultlib=libphobos2.so"
			],
			"name": "linux-dynamic",
			"platforms": [
				"linux"
			],
			"targetType": "dynamicLibrary"
		},
		{
			"dependencies": {
				"arsd-official:image_files": "~>11.5.3",
				"elemi": "~>1.2.2"
			},
			"dflags": [
				"-verrors=0"
			],
			"importPaths": [
				"tests",
				"tools",
				"source"
			],
			"libs": [
				"raylib"
			],
			"name": "unittest",
			"sourceFiles": [
				"tools/silly.d"
			],
			"sourcePaths": [
				"tests",
				"source"
			],
			"targetType": "library",
			"versions": [
				"Fluid_HeadlessOutput",
				"Fluid_TestSpace",
				"Fluid_SVG"
			]
		},
		{
			"dflags-ldc": [
				"--ftime-trace",
				"--ftime-trace-file=build/trace.json"
			],
			"name": "time-trace",
			"targetType": "library"
		}
	],
	"copyFiles-windows": [
		"$PACKAGE_DIR/libraries/windows/freetype.dll",
		"$PACKAGE_DIR/libraries/windows/raylib.dll"
	],
	"copyright": "Copyright © 2021–2024, Artha",
	"dependencies": {
		"arsd-official:image_files": {
			"optional": true,
			"version": "~>11.5.3"
		},
		"bindbc-freetype": "~>1.1.1",
		"bindbc-loader": "~>1.1.3",
		"elemi": {
			"optional": true,
			"version": "~>1.2.2"
		},
		"event-pipe": ">=1.0.0-rc.1 <1.1.0-0",
		"optional": "~>1.3.0",
		"raylib-d": {
			"default": true,
			"optional": true,
			"version": "~>5.0"
		}
	},
	"description": "Flexible, declarative library for graphical user interfaces (GUI, UI)",
	"excludedSourceFiles": [
		"source/fluid/module_view.d"
	],
	"lflags-linux": [
		"-L$PACKAGE_DIR/libraries/ubuntu"
	],
	"lflags-osx": [
		"-L$PACKAGE_DIR/libraries/macos"
	],
	"lflags-windows": [
		"/LIBPATH:$PACKAGE_DIR/libraries/windows"
	],
	"libs": [
		"freetype"
	],
	"license": "BSL",
	"name": "fluid",
	"stringImportPaths": [
		"resources"
	],
	"subConfigurations": {
		"bindbc-freetype": "staticBC"
	},
	"subPackages": [
		"./tour",
		{
			"name": "init-exec",
			"sourceFiles": [
				"source/fluid/dub_template.d"
			],
			"sourcePaths": [],
			"targetPath": "build",
			"targetType": "executable",
			"versions": [
				"Fluid_InitExec"
			]
		},
		{
			"configurations": [
				{
					"name": "linux-dynamic",
					"platforms": [
						"linux"
					],
					"subConfigurations": {
						"fluid": "linux-dynamic"
					},
					"targetType": "library"
				},
				{
					"name": "default",
					"targetType": "library"
				}
			],
			"dependencies": {
				"fluid": {
					"path": "."
				},
				"fluid-tree-sitter": "~>0.2.0",
				"fluid-tree-sitter:d": "~>0.2.0"
			},
			"environments": {
				"BINDBC_FREETYPE_PACKAGE_DIR": "$BINDBC_FREETYPE_PACKAGE_DIR",
				"BINDBC_LOADER_PACKAGE_DIR": "$BINDBC_LOADER_PACKAGE_DIR",
				"BOLTS_PACKAGE_DIR": "$BOLTS_PACKAGE_DIR",
				"OPTIONAL_PACKAGE_DIR": "$OPTIONAL_PACKAGE_DIR"
			},
			"environments-linux": {
				"FLUID_LIBPATH": "$FLUID_PACKAGE_DIR/libraries/ubuntu"
			},
			"environments-osx": {
				"FLUID_LIBPATH": "$FLUID_PACKAGE_DIR/libraries/macos"
			},
			"environments-windows": {
				"FLUID_LIBPATH": "$FLUID_PACKAGE_DIR/libraries/windows"
			},
			"name": "module-view",
			"sourceFiles": [
				"source/fluid/module_view.d"
			],
			"sourcePaths": [],
			"targetPath": "build",
			"targetType": "library",
			"versions": [
				"Fluid_ModuleView"
			]
		}
	],
	"targetPath": "build"
}