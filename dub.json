			"dependencies": {
				"libdparse": "~>0.23.2",
				"fluid": { "path": "." }
			},
			"name": "showcase",
			"libs": [
				"raylib"
			],
			"preBuildCommands": [
				"echo Deprecated: fluid:showcase has been renamed to fluid:tour"
			],
			"sourcePaths": [
				"tour"
			],
			"targetPath": "build",
			"targetName": "fluid-tour",
			"targetType": "executable"
		},
		{
			"name": "init-exec",
			"versions": ["Fluid_InitExec"],
			"sourceFiles": ["source/fluid/dub_template.d"],
			"sourcePaths": [],
			"targetType": "executable",
			"targetPath": "build"
		},
		{
			"name": "module_view",
			"versions": ["Fluid_ModuleView"],
			"sourceFiles": ["source/fluid/module_view.d"],
			"sourcePaths": [],
			"targetType": "library",
			"targetPath": "build",
			"dependencies": {
				"fluid-tree-sitter": "~>0.1.3",
				"fluid-tree-sitter:d": "~>0.1.3"
			}
		}
	],
	"targetPath": "build",
	"targetType": "library"
